
@model BrandCarusel
    <div class="content-body">
        <!--Form State Start-->
        <div class="col-12 mb-30">
            <div class="row mbn-15">
                <div class="col-lg-12 mb-15">
                    <form method="post" enctype="multipart/form-data">
                        <div class="form-group mt-15">
                            <label asp-for="Photo">Image:</label>
                            <input asp-for="Photo" accept="image\*" style="width:400px" /><br>
                            <span asp-validation-for="Photo" class="text-danger"></span>
                        </div>
                        <div class="mt-25">
                            <input type="submit" class="btn btn-outline-primary" value="Create" />
                        </div>
                    </form>
                    <div class="row mt-15">
                        <div class="col-12">
                            <a asp-action="Index" class="btn btn-outline-info"><span class="fa fa-arrow-left"></span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

